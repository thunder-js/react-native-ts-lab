{
  "name": "TsLab",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:ios": "yarn haul start --platform ios",
    "start:android": "yarn haul start --platform android",
    "bundle:ios": "haul bundle --platform ios --entry-file index.js --bundle-output dist/ios/main.jsbundle --assets-dest dist/assets --dev false",
    "bundle:android": "haul bundle --platform android --entry-file index.js --bundle-output dist/android/main.jsbundle --assets-dest dist/assets --dev false",
    "release-push:ios": "yarn run bundle:ios && code-push release TsLab dist/ios 1.0.0",
    "release-push:android": "yarn run bundle:android && code-push release TsLab-Android dist/android 1.0.0",
    "beta-store:ios": "script/publish-store ios beta",
    "beta-store:android": "script/publish-store android beta",
    "release-store:ios": "script/publish-store ios release",
    "release-store:android": "script/publish-store android release",
    "storybook:ios": "storybook start -p 7007 --haul webpack.haul.storybook.js --platform ios",
    "storybook:android": "storybook start -p 7007 --haul webpack.haul.storybook.js --platform android",
    "test": "jest",
    "test:e2e": "detox test"
  },
  "dependencies": {
    "react": "16.0.0",
    "react-native": "0.51.0",
    "react-native-code-push": "^5.2.1",
    "react-native-navigation": "^1.1.328"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.3",
    "@storybook/addon-links": "^3.3.3",
    "@storybook/addon-storyshots": "^3.3.3",
    "@storybook/react-native": "^3.3.3",
    "@types/jest": "^22.0.0",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-react-native": "4.0.0",
    "detox": "^6.0.4",
    "haul": "^1.0.0-beta.11",
    "jest": "22.0.4",
    "jest-static-stubs": "^0.0.1",
    "mocha": "^4.1.0",
    "prop-types": "^15.6.0",
    "react-dom": "16.0.0",
    "react-test-renderer": "16.0.0",
    "ts-jest": "^22.0.0",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "testPathIgnorePatterns": [
      "node_modules",
      "e2e"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      ".+\\.(png)$": "jest-static-stubs/$1"
   }
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/TsLab.app",
        "build": "xcodebuild -project ios/TsLab.xcodeproj -scheme TsLab -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    }
  }
}
